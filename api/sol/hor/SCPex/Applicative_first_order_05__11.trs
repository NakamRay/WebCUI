  (D((bangplus(X,Y)))) -> (bangplus((D(X)),(D(Y)))),
  (D((bangtimes(U,V)))) -> (bangplus((bangtimes(V,(D(U)))),(bangtimes(U,(D(V)))))),
  (D((bangminus(W,P)))) -> (bangminus((D(W)),(D(P)))),
