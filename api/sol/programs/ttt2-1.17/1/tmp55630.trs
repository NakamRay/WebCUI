(VAR P Q R )
(RULES
  and(P,all(Q)) -> all(and(P,Q))
  and(all(Q),P) -> all(and(Q,P))
  or(P,all(Q)) -> all(or(P,Q))
  or(all(Q),P) -> all(or(Q,P))
  and(P,ex(Q)) -> ex(and(P,Q))
  and(ex(Q),P) -> ex(and(Q,P))
  or(P,ex(Q)) -> ex(or(P,Q))
  or(ex(Q),P) -> ex(or(Q,P))
  not(all(Q)) -> ex(not(Q))
  not(ex(Q)) -> all(not(Q))
  and(P,R) -> and(R,P)
  or(P,R) -> or(R,P))