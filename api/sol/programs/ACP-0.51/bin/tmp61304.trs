(VAR X Y Z )
(RULES
  xpls(X,0) -> X
  xpls(X,s(Y)) -> s(xpls(X,Y))
  xpls(0,Y) -> Y
  xpls(s(X),Y) -> s(xpls(X,Y))
  xmul(X,0) -> 0
  xmul(X,s(Y)) -> xpls(xmul(X,Y),X)
  xmul(0,Y) -> 0
  xmul(s(X),Y) -> xpls(xmul(X,Y),Y)
  xpls(xpls(X,Y),Z) -> xpls(X,xpls(Y,Z))
  xpls(X,Y) -> xpls(Y,X)
  xmul(X,Y) -> xmul(Y,X))